<?xml version="1.0" encoding="utf-8"?>
<vector xmlns:android="http://schemas.android.com/apk/res/android"
    android:width="400dp"
    android:height="600dp"
    android:viewportWidth="400"
    android:viewportHeight="600">
    
    <!-- Multiple wave layers for natural water effect -->
    
    <!-- Wave layer 3 (deepest, darkest) - Most subtle -->
    <path
        android:fillColor="#E8DDD0"
        android:pathData="M0,75 Q50,55 100,60 T200,65 T300,60 T400,70 L400,600 L0,600 Z" />
    
    <!-- Wave layer 2 (middle) - More pronounced -->
    <path
        android:fillColor="#EDE4D8"
        android:pathData="M0,65 Q60,40 120,50 T240,55 T360,45 T400,60 L400,600 L0,600 Z" />
    
    <!-- Wave layer 1 (front, lightest) - Main visible wave -->
    <path
        android:fillColor="#F5EDE3"
        android:pathData="M0,55 Q50,30 100,40 Q150,50 200,35 Q250,20 300,40 Q350,55 400,50 L400,600 L0,600 Z" />
    
    <!-- Top most layer - smooth blend -->
    <path
        android:fillColor="#F9F5F0"
        android:pathData="M0,50 Q40,25 80,35 Q120,45 160,30 Q200,15 240,30 Q280,45 320,35 Q360,25 400,45 L400,600 L0,600 Z" />
        
</vector>

